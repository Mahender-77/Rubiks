{
  "expo": {
    "name": "Rubiks",
    "slug": "rubiks-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "extra": {
      "router": {},
      "eas": {
        "projectId": "181f37a6-7b40-4627-98c4-0fbd7238f22f"
      },
      "API_BASE_URL": "http://145.223.21.150:5001/api"
    },
    "ios": {
      "supportsTablet": true
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-web-browser",
      [
        "expo-build-properties",
        {
          "android": {
            "usesCleartextTraffic": true,
            "allowBackup": false,
            "networkSecurityConfig": "@xml/network_security_config"
